{
  "manifest_version": 3,
  "name": "Rust Companion Authenticator",
  "version": "0.0.1",
  "background": {
    "service_worker": "background.ts",
    "type": "module"
  },
  "content_scripts": [
    {
      "matches": [
        "https://companion-rust.facepunch.com/app*"
      ],
      "js": [
        "auth-hook-content-script.ts"
      ],
      "run_at": "document_start",
      "world": "MAIN"
    },
    {
      "matches": [
        "https://companion-rust.facepunch.com/app*"
      ],
      "js": [
        "content-script.ts"
      ],
      "css": [
        "content-script.css"
      ]
    }
  ]
}